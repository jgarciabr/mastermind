{% extends 'base.html.twig' %}

{% block body %}
    <h1>Jugadas list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Num</th>
                <th>Date</th>
                <th>Code</th>
                <th>Result</th>
                <th>Partidaid</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for jugada in jugadas %}
            <tr>
                <td><a href="{{ path('jugada_show', { 'id': jugada.id }) }}">{{ jugada.id }}</a></td>
                <td>{{ jugada.num }}</td>
                <td>{% if jugada.date %}{{ jugada.date|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ jugada.code1 }}</td>
                <td>{{ jugada.code2 }}</td>
                <td>{{ jugada.code3 }}</td>
                <td>{{ jugada.code4 }}</td>
                <td>{{ jugada.code5 }}</td>
                <td>{{ jugada.code6 }}</td>

                <td>{{ jugada.result }}</td>
                <td>{{ jugada.getPartida.name }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('jugada_show', { 'id': jugada.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('jugada_edit', { 'id': jugada.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
